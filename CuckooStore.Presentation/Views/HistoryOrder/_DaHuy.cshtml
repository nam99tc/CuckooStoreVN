@model IEnumerable<CuckooStore.Models.Order>
@if (Model.Count() > 0)
{
    foreach (var item in Model)
    {
        Html.RenderAction("HistoryDetail", "HistoryOrder", new { id = item.OrderID });
    }
}
else
{
    <div class="how-pos2 p-lr-15-md">
        <div style="text-align: center; padding: 150px;"> <img src="~/wwwroot/images/emtyOrder.png"></div>
    </div>
}
